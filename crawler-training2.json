{
  "spider_name": "unified-us-ut-stc-news-releases-training",
  "api_table": "legal_api_docs",
  "meta_table": "agency_updates",
  "jurisdiction": "US-UT",
  "agency": "Utah State Tax Commission - News Releases",
  "agency_id": 1900004,
  "short_name": "UT-STC",
  "category": "News Release",
  "base_url": "https://tax.utah.gov/",
  "content_url": "https://tax.utah.gov/commission-office/news",
  "specification": "https://docs.google.com/document/d/10wk8s621SwIdtRNZ8FMuRtaLvPR7N1bhH_0Zpi43iQg/edit",
  "configurations": {
    "steps": {
      "start": {
        "description": "Starting step that loads main web-page.",
        "save_document": false,
        "next_step": ["articles_step"]
      },
      "item_step": {
        "description": "Getting metadata of the current article",
        "save_document": true,
        "doc": {
          "pdf_url": {
            "is_text": false,
            "is_url": true,
            "is_single": true,
            "is_sibling_tag": false,
            "is_sibling_text": false,
            "is_date": false,
            "path": ["td.column-3 > a"]
          },
          "title": {
            "is_text": true,
            "is_url": false,
            "is_single": true,
            "is_sibling_tag": false,
            "is_sibling_text": false,
            "is_date": false,
            "path": ["td.column-4"]
          },
          "pub_date": {
            "is_text": true,
            "is_url": false,
            "is_single": true,
            "is_sibling_tag": false,
            "is_sibling_text": false,
            "is_date": true,
            "path": ["td.column-2"]
          }
        },
        "parent_context": "articles_step"
      },
      "articles_step": {
        "description": "live demo for list",
        "save_document": true,
        "context": {
          "is_url": false,
          "is_text": false,
          "is_single": false,
          "is_sibling_tag": false,
          "combine_context": false,
          "path": ["tbody.row-hover > tr"]
        },
        "doc": {
          "title": {
            "path": ["tbody.row-hover > tr"],
            "code_filter": "results if results else ['Reached article step but nothing was here']"
          },
          "html": {
            "path": "body"
          },
          "web_url": {
            "path": "body",
            "is_current_url": true
          },
          "incomplete_pub_date": true
        },
        "next_step": ["item_step"]
      }
    }
  }
}
